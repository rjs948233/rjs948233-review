package list;

import java.util.ArrayList;

public class ArrayList4 {

	public static void main(String[] args) {
	   ArrayList<Integer> list=new ArrayList<Integer>();
       System.out.println(list);
       
       	for(int i=0;i<20;i++) {
       			list.add(i*10);
       	}
	
       	System.out.println(list);
	    int index=0;
       	while(!list.isEmpty())
       		index += 1;
       	for(Integer li:list) {
       		System.out.println(list.get(index));
       	}
    }
}