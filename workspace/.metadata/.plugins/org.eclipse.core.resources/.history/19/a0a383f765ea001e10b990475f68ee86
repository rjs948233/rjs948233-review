package finalTest;

public class finalTest {

	
	
	public static void main(String[] args) {
		int maxLength = 10; //변수
		System.out.println(maxLength);
		maxLength = 30; //변수의 값 변경 가능
		System.out.println(maxLength);
		
		//클래스 내부에서 상수를 선언하는 방법
		final int MAX_LENGTH = 100;
		System.out.println(MAX_LENGTH);
//		MAX_LENGTH = 200;
		
	}
}
























